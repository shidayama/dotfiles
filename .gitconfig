[user]
  name  = SET_ME_LOCALLY
  email = SET_ME_LOCALLY
[color]
  ui          = auto
  status      = auto
  diff        = auto
  branch      = auto
  interactive = auto
  grep        = auto
[push]
  default = upstream
[core]
  excludesfile = ~/.gitignore_global
  autocrlf     = input
  editor       = vim
[alias]
  st     = status
  sw     = show
  sh     = stash
  shp    = stash pop
  f      = fetch
  p      = push
  po     = push origin
  pu     = push -u origin
  pl     = pull
  a      = add
  aa     = add --all
  c      = commit
  cm     = commit -m
  ca     = commit -a
  em     = commit -a --allow-empty-message -m ''
  camend = commit --amend -m
  co     = checkout
  b      = branch
  srb    = svn rebase
  sdc    = svn dcommit
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /usr/local/Caskroom/sourcetree/1.8.1/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[init]
  templatedir = ~/.git_template
